<% layout('layouts/boilerplate')%>
    <h1>All Campgrounds</h1>
    <div>
        <a href="/campgrounds/new">Add Campground</a>
    </div>
    <ul>
        <% for (let camp of allCamps) { %>
            <li><a href="/campgrounds/<%=camp._id%>">
                    <%= camp.title %>
                </a></li>
            <% } %>
    </ul>